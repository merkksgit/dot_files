{
    "terminal.integrated.fontSize": 18,
    "terminal.integrated.fontFamily": "JetBrainsmono Nerd Font ",
    "editor.fontSize": 18,
    "editor.fontFamily": "JetBrainsmono Nerd Font",
    "workbench.startupEditor": "none",
    "workbench.colorTheme": "Tokyo Night Storm",
    "code-runner.runInTerminal": true,
    "workbench.iconTheme": "material-icon-theme",
    "git.openRepositoryInParentFolders": "always",
    "indentRainbow.colors": [
        "rgb(122, 162, 247,0.08)",
        "rgba(125, 207, 255,0.08)",
        "rgba(192, 202, 245,0.08)",
        "rgba(180, 249, 248,0.08)"
    ],
    "editor.minimap.enabled": false,
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "vscode-neovim.useWSL": true,
    "vscode-neovim.neovimExecutablePaths.linux": "/opt/nvim-linux64/bin/nvim",
    "editor.lineNumbers": "relative",
    "editor.cursorSurroundingLines": 5,
    "workbench.colorCustomizations": {
        "editor.findMatchBackground": "#7dcfff50",
        "editor.findMatchHighlightBackground": "#bb9af750",
        "editor.selectionBackground": "#7dcfff50",
        "editor.selectionHighlightBackground": "#bb9af750",
        "statusBar.background": "#7AA2F7",
        "statusBar.noFolderBackground": "#7AA2F7",
        "statusBar.debuggingBackground": "#7AA2F7",
        "statusBar.foreground": "#24283B",
        "statusBar.debuggingForeground": "#24283B",
        "editor.linkedEditingBackground": "#ff0000",
        "editor.wordHighlightBackground": "#7aa2f783"
    },
    "vim.statusBarColors.easymotioninputmode": "",
    "vim.statusBarColorControl": true,
    "vim.statusBarColors.insert": [
        "#FF757F",
        "#24283B"
    ],
    "vim.statusBarColors.commandlineinprogress": [
        "#C3E88D",
        "#24283B"
    ],
    "vim.statusBarColors.normal": [
        "#24283B",
        "#ffffff"
    ],
    "vim.statusBarColors.visual": [
        "#BB9AF7",
        "#24283B"
    ],
    "vim.statusBarColors.visualline": [
        "#7AA2F7",
        "#24283B"
    ],
    "vim.statusBarColors.visualblock": [
        "#FF9E64",
        "#24283B"
    ],
    "vim.statusBarColors.searchinprogressmode": [
        "#545C7E",
        "#ffffff"
    ],
    "liveServer.settings.ChromeDebuggingAttachment": false,
    "liveServer.settings.CustomBrowser": "chrome",
    "liveServer.settings.donotShowInfoMsg": true,
    //Smart relative lines
    "vim.smartRelativeLine": true,
    //Word Separators
    "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?",
    //Yank to System Clipboard 
    "vim.useSystemClipboard": true,
    //Highliht yanked text
    "vim.highlightedyank.enable": true,
    "vim.highlightedyank.color": "#bb9af750",
    "vim.highlightedyank.duration": 750,
    "editor.semanticHighlighting.enabled": true
}